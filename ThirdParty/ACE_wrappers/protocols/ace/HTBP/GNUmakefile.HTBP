	$(ACE_ROOT)/bin/ace_install_pkgconfig.pl ACE_HTBP.pc.in --destdir "$(DESTDIR)" --prefix $(INSTALL_PREFIX) --libdir $(INSTALL_LIB) --libs "$(LIBS)" --version $(GNUACE_PROJECT_VERSION)
